@model ShippingCalculator.Models.Carrier

<h2>Create New Carrier</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MaxLength"></label>
        <input asp-for="MaxLength" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MaxWidth"></label>
        <input asp-for="MaxWidth" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MaxHeight"></label>
        <input asp-for="MaxHeight" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MaxWeight"></label>
        <input asp-for="MaxWeight" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="MaxCubic"></label>
        <input asp-for="MaxCubic" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
